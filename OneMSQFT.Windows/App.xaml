<prism:MvvmAppBase 
    xmlns:prism="using:Microsoft.Practices.Prism.StoreApps"
    x:Class="OneMSQFT.Windows.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:OneMSQFT.Windows">
    <prism:MvvmAppBase.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="../Styles/StandardStyles.xaml"/>
                <ResourceDictionary Source="../Styles/generic.xaml"/>
                <ResourceDictionary Source="../Styles/OMSQFTStyles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            
            <portable:PortableStrings xmlns:portable="using:OneMSQFT.Common" x:Key="PortableStrings"/>
            
            <DataTemplate x:Key="OMSQFTAppBarButtonTemplate">
                <Grid Height="100" Width="100" Background="Red">
                    <TextBlock Text="{Binding Name}"></TextBlock>
                </Grid>
            </DataTemplate>

        </ResourceDictionary>
    </prism:MvvmAppBase.Resources>

</prism:MvvmAppBase>
