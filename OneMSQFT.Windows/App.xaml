<prism:MvvmAppBase 
    xmlns:prism="using:Microsoft.Practices.Prism.StoreApps"
    x:Class="OneMSQFT.WindowsStore.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"    
    RequestedTheme="Dark">
    <prism:MvvmAppBase.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="../Styles/StandardStyles.xaml"/>
                <!--<ResourceDictionary Source="../Styles/MMPPFstyles.xaml"/> // if we need to style the player activate these -->
                <!--<ResourceDictionary Source="../Styles/generic.xaml"/>-->
                <ResourceDictionary Source="../Styles/OMSQFTStyles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            
            <portable:PortableStrings xmlns:portable="using:OneMSQFT.Common" x:Key="PortableStrings"/>
        </ResourceDictionary>
    </prism:MvvmAppBase.Resources>

</prism:MvvmAppBase>
